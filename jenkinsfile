pipeline {
    agent any

    stages {
        stage('clone') {
            steps {
                git branch: 'main', url: 'https://github.com/soumyaa0/terraform_CICD.git'
            }
        }
        stage('init') {
            steps {
                sh 'terraform init'
                sh 'terraform plan'
            }
        }
    }
}
